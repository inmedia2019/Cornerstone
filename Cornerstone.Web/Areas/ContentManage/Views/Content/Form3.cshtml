@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script>
    layui.use(['jquery', 'form', 'laydate','xmSelect', 'optimizeSelectOption', 'common'], function () {
        var form = layui.form,
            $ = layui.$,
            common = layui.common,
            laydate = layui.laydate,
            upload = layui.upload,
            xmSelect = layui.xmSelect;

        var keyValue = $.request("keyValue");
        var itemId = $.request("itemId");
        //权限字段
        common.authorizeFields('adminform');

        laydate.render({
            elem: '#F_MeetingStartTime',
            format: 'yyyy/MM/dd HH:mm:ss',
            type:'datetime',
        });
        laydate.render({
            elem: '#F_MeetingEndTime',
            format: 'yyyy/MM/dd HH:mm:ss',
            type: 'datetime',
        });
        laydate.render({
            elem: '#F_PublishTime',
            format: 'yyyy/MM/dd HH:mm:ss',
            type: 'datetime',
        });
         var channelSelect= xmSelect.render({
              el: '#channelSelect',
            autoRow: true,
            filterable: true,
            direction: 'down',
            tree: {
                show: true,
                showFolderIcon: true,
                showLine: true,
                indent: 20,
                strict: false,
                clickExpand: true,
                clickCheck: true,
                expandedKeys: true
            },
            toolbar: {
                show: true,
                list: ['ALL', 'REVERSE', 'CLEAR']
            },
            filterable: true,
            height: 'auto',
            data: function () {
                return  @Html.Raw(ViewBag.channel)
            }
        });
        var tagSelect= xmSelect.render({
            el: '#tagSelect',
            autoRow: true,
            filterable: true,
            direction: 'down',
            tree: {
                show: true,
                showFolderIcon: true,
                showLine: true,
                indent: 20,
                strict: false,
                clickExpand: true,
                clickCheck: true,
                expandedKeys: true
            },
            toolbar: {
                show: true,
                list: ['ALL', 'REVERSE', 'CLEAR']
            },
            filterable: true,
            height: 'auto',
            data: function () {
                return  @Html.Raw(ViewBag.lable)
            }
        });
        @*var authorSelect =xmSelect.render({
            el: '#authorSelect',
            autoRow: true,
            filterable: true,
            toolbar: {
                show: true,
                list: ['ALL', 'REVERSE', 'CLEAR']
            },
            data: @Html.Raw(ViewBag.expert),
        });*@
        var event = $.request("event");
        wcLoading.close();
        //此处需修改
        //类型为时间时
        //laydate.render({
            //elem: '#F_Birthday'
            //, btns: ['clear', 'now']
            //, trigger: 'click',
            //format: 'yyyy-MM-dd',
        //});
        $(function () {
            initControl();

            if (event == "edit") {
                var newsdata = JSON.parse('@Html.Raw(ViewBag.Content)');
                common.val('adminform', newsdata);
                var Description2 = newsdata.F_ShareDes;
              //  var Description3 = newsdata.F_WonderfulContent;
                if (!!newsdata.ImgUrl) {
                    ImgUrlPic.src = newsdata.ImgUrl;
                }
                if (Description2 != null) {
                    var editor2 = UE.getEditor("F_ShareDes");
                    editor2.ready(function () {
                        editor2.setContent(Description2);
                    });
                }
             //  if (Description3 != null) {
               // var editor3 = UE.getEditor("F_WonderfulContent");
              //  editor3.ready(function () {
                //    editor3.setContent(Description3);
              //  });
              //  }
                if (newsdata.F_SubChannelId != null) {
                    var channelarr = newsdata.F_SubChannelId.split(',');
                    channelSelect.setValue(channelarr);
                }
                if (newsdata.F_Tags != null) {
                    var tagarr = newsdata.F_Tags.split(',');
                    tagSelect.setValue(tagarr);
                }
                //if (newsdata.F_Author != null) {
                //    var authorrr = newsdata.F_Author.split(',');
                //    authorSelect.setValue(authorrr);
                //}
                if (newsdata.F_ShareTitle != null) {

                    var imageHtmls = "";
                    var temp = newsdata.F_ShareTitle.split(',');

                    for (var i in temp) {

                        if (temp[i] != "")
                            imageHtmls += '<img onclick="previewImg(this)" src="' + temp[i] + '" />';
                    }

                    $(".F_ScoreTypePic").html(imageHtmls);
                    $("#F_ScoreTypeImgshow").removeAttr("class", "layui-hide");
                }
            }
            if ($("#F_CoverImage").val() != "") {
                $("#F_ImgUrlPic").attr("src", $("#F_CoverImage").val());
                $("#imgshow").removeAttr("class", "layui-hide");
            }

            if ($("#F_SharePic").val() != "") {
                $("#F_PictureUrlPic").attr("src", $("#F_SharePic").val());
                $("#F_PictureUrlImgshow").removeAttr("class", "layui-hide");
            }

            if ($("#F_LiveBroadcastId").val() != "") {
                $("#F_LiveBroadcastIdPic").attr("src", $("#F_LiveBroadcastId").val());
                $("#F_LiveBroadcastIdImgshow").removeAttr("class", "layui-hide");
            }


            //if ($("#F_ShareTitle").val() != "") {

            //    var imageHtmls = "";
            //    var temp = $("#F_ShareTitle").val().split(',');
            //    for (var i in temp) {

            //        if (temp[i] != "")
            //            imageHtmls += '<img onclick="previewImg(this)" src="' + temp[i].src + '" />';
            //    }

            //    $(".F_ScoreTypePic").html(imageHtmls);
            //    $("#F_ScoreTypeImgshow").removeAttr("class", "layui-hide");
            //}


            if ($("#F_Video").val() != "") {

                $('.videoShow').show();

            }


           form.render();
       });

       function initControl() {
           $("#F_ChannelId").bindSelect({
               url: "@(Cornerstone.Code.GlobalContext.SystemConfig.VirtualDirectory)/ContentManage/Channel/GetTreeSelectJson"
           });
           $("#F_ChannelId").val('@(ViewBag.ChannelId)');


           $('#F_Video').on("input propertychange", function () {

               $('.video').show();

           });

       }
        //select验证
        form.verify({
            required: function (value, item) {
                var msg = "必填项不能为空";
                value = $.trim(value);
                var isEmpty = !value || value.length < 1;
                // 当前验证元素是select且为空时,将页面定位至layui渲染的select处，或自定义想定位的位置
                if (item.tagName == 'SELECT' && isEmpty) {
                    $("html").animate({
                        scrollTop: $(item).siblings(".layui-form-select").offset().top - 74
                    }, 50);
                }
                if (isEmpty) {
                    return msg;
                }
            }
        });
       //监听提交
        form.on('submit(saveBtn)', function (data) {
            // 单击之后提交按钮不可选,防止重复提交
            $('.site-demo-active').addClass('layui-btn-disabled');
            $('.site-demo-active').attr('disabled', 'disabled');
            var postData = data.field;
            if (!postData["F_ShareDes"]) postData["F_ShareDes"] = UE.getEditor('F_ShareDes').getContent();
           // if (!postData["F_WonderfulContent"]) postData["F_WonderfulContent"] = UE.getEditor('F_WonderfulContent').getContent();
            if (!postData["F_EnabledMark"]) postData["F_EnabledMark"] = false;
            if (!postData["F_IsRecommend"]) postData["F_IsRecommend"] = false;
            if (!postData["F_IsAuthorization"]) postData["F_IsAuthorization"] = false;
            if (!postData["F_IsLive"]) postData["F_IsLive"] = false;
            if (!postData["F_IsTop"]) postData["F_IsTop"] = false;
            if (!postData["F_Status"]) postData["F_Status"] = false;
            postData["F_ItemId"] = itemId;
            //postData["F_SubChannelId"] = channelSelect.getValue('valueStr');
            //postData["F_Tags"] = tagSelect.getValue('valueStr');
            //postData["F_Author"] = authorSelect.getValue('valueStr');
            common.submitForm({
                url: event == "edit" ? "@(Cornerstone.Code.GlobalContext.SystemConfig.VirtualDirectory)/ContentManage/Content/SubmitForm?keyValue=" + keyValue : "@(Cornerstone.Code.GlobalContext.SystemConfig.VirtualDirectory)/ContentManage/Content/SubmitForm",
                param: postData,
                close: false,
                success: function (result) {
                    //common.reloadIframe("@(Cornerstone.Code.GlobalContext.SystemConfig.VirtualDirectory)/ContentManage/Content/Index", 'data-search-btn');
                    var currentUrl = window.location.href;
                    var iframes = top.$(".layui-show>iframe");
                    if (currentUrl.indexOf("event=edit") >= 0) {
                        common.reloadIframe("@(Cornerstone.Code.GlobalContext.SystemConfig.VirtualDirectory)/ContentManage/Content/Index", 'data-search-btn');
                        iframes[0].src = currentUrl;
                    }
                    else {
                        iframes[0].src = currentUrl.substring(0, currentUrl.indexOf("?")) + "?event=edit&keyValue=" + result.keyValue;

                    }
                }
            })
            return false;
        });

        $("#NF-lan-en").on("click", function () {
            var iframes = top.$(".layui-show>iframe");
            $.get({
                data: {"lan":"1"},
                url: '@(Cornerstone.Code.GlobalContext.SystemConfig.VirtualDirectory)/ContentManage/Block/SetLan',
                success: function (data) {
                    iframes[0].src = window.location.href;
                }
            })

        });

        $("#NF-lan-cn").on("click", function () {
            var iframes = top.$(".layui-show>iframe");
            $.get({
                data: {"lan":"0"},
                url: '@(Cornerstone.Code.GlobalContext.SystemConfig.VirtualDirectory)/ContentManage/Block/SetLan',
                success: function (data) {
                    iframes[0].src = window.location.href;
                }
            })

        });
   });
</script>

<body>
    <center>
        <div style=" margin: 10px;" class="showlan">

            <button id="NF-lan-cn" authorize="yes" class="layui-btn layui-btn-sm layui-btn-warm  " style="height:30px; padding:0 25px;" lay-submit lay-filter="lan-cn"> <i class="fa fa-flag"></i>中文版</button>
            <button id="NF-lan-en" authorize="yes" class="layui-btn layui-btn-sm  " lay-submit style="height:30px; padding:0 25px;" lay-filter="lan-en"> <i class="fa fa-flag-checkered"></i>英文版</button>
        </div>

    </center>
    <div class="layuimini-container">
        <div class="layuimini-main" style="margin-bottom: 80px;">
            <div class="layui-form layuimini-form" lay-filter="adminform">
                <div class="layui-form-item layui-hide">
                    <label class="layui-form-label required">栏目</label>
                    <div class="layui-input-block">
                        <input type="hidden" id="F_LanInfoId" name="F_LanInfoId" value="">
                        <select id="F_ChannelId" name="F_ChannelId" lay-filter="F_ChannelId" lay-search>
                            <option value="">请选择栏目</option>
                        </select>
                    </div>
                </div>
                <div class="layui-form-item layui-hide">
                    <label class="layui-form-label required">标题</label>
                    <div class="layui-input-block">
                        <input type="hidden" id="F_HitCount" name="F_HitCount" value="0">
                        <input type="hidden" id="F_LikeCount" name="F_LikeCount" value="0">
                        <input type="hidden" id="F_CommentCount" name="F_CommentCount" value="0">
                        <input type="hidden" id="F_SubscriptionCount" name="F_SubscriptionCount" value="0">
                        <input type="hidden" id="F_FavoritesCount" name="F_FavoritesCount" value="0">
                        <input type="hidden" id="F_HitBackToCount" name="F_HitBackToCount" value="0">
                        <input type="text" id="F_Titile" name="F_Titile" autocomplete="off" maxlength="200" lay-verify="required" class="layui-input">
                    </div>
                </div>


                <div class="layui-form-item layui-hide">
                    <label class="layui-form-label">视频图片</label>
                    <div class="layui-input-block" style="padding-right: 110px;">
                        <input type="text" class="layui-input" name="F_CoverImage" id="F_CoverImage" maxlength="255">
                        <button type="button" class="layui-btn" style="position: absolute;top: 0;right: 0;cursor: pointer;" id="j_upload_img_btn">
                            <i class="layui-icon"></i>上传图片
                        </button>
                        <ul id="upload_img_wrap"></ul>
                        <textarea id="uploadEditor" style="display: none;"></textarea>
                        <span style="color:red">(2256*444)上传图片不能大于1M</span>
                    </div>
                </div>

                <div class="layui-form-item layui-hide" id="imgshow">
                    <label class="layui-form-label"></label>
                    <div class="layui-input-block">
                        <img id="F_ImgUrlPic" width="200" height="200" onclick="previewImg(this)" />
                    </div>
                </div>

                <div class="layui-form-item layui-hide">
                    <label class="layui-form-label">视频</label>
                    <div class="layui-input-block" style="padding-right: 110px;">
                        <input type="text" class="layui-input" name="F_Video" id="F_Video" maxlength="255">
                        <button type="button" class="layui-btn" style="position: absolute;top: 0;right: 0;cursor: pointer;" id="j_upload_img_btn1">
                            <i class="layui-icon"></i>上传视频
                        </button>
                        <ul id="upload_img_wrap"></ul>
                        <textarea id="uploadEditor1" style="display: none;"></textarea>

                    </div>
                </div>

                <div class="layui-form-item layui-hide" id="videoShow">
                    <label class="layui-form-label"></label>
                    <div class="layui-input-block">
                        <video src="" width="500px" height="300px" controls></video>
                    </div>
                </div>


                <div class="layui-form-item layui-hide">
                    <label class="layui-form-label">主体背景图</label>
                    <div class="layui-input-block" style="padding-right: 110px;">
                        <input type="text" class="layui-input" name="F_SharePic" id="F_SharePic" maxlength="255">
                        <button type="button" class="layui-btn" style="position: absolute;top: 0;right: 0;cursor: pointer;" id="j_upload_img_btnbg">
                            <i class="layui-icon"></i>上传图片
                        </button>
                        <ul id="upload_img_wrap"></ul>
                        <textarea id="uploadEditorBg" style="display: none;"></textarea>
                        <span style="color:red">(1920*1080)上传图片不能大于1M</span>
                    </div>
                </div>
                <div class="layui-form-item layui-hide" id="F_PictureUrlImgshow">
                    <label class="layui-form-label"></label>
                    <div class="layui-input-block">
                        <img id="F_PictureUrlPic" onclick="previewImg(this)" />
                    </div>
                </div>

                <div class="layui-form-item layui-hide">
                    <label class="layui-form-label">主体文字图</label>
                    <div class="layui-input-block" style="padding-right: 110px;">
                        <input type="text" class="layui-input" name="F_ShareTitle" id="F_ShareTitle" maxlength="255">
                        <button type="button" class="layui-btn" style="position: absolute;top: 0;right: 0;cursor: pointer;" id="j_upload_img_btnFont">
                            <i class="layui-icon"></i>上传图片
                        </button>
                        <ul id="upload_img_wrap"></ul>
                        <textarea id="uploadEditorFont" style="display: none;"></textarea>
                        <span style="color:red">(825*381)上传图片不能大于1M</span>
                    </div>
                </div>

                <div class="layui-form-item layui-hide" id="F_ScoreTypeImgshow">
                    <label class="layui-form-label"></label>
                    <div class="layui-input-block F_ScoreTypePic">
                        @*<img id="F_ScoreTypePic" width="200" height="200" onclick="previewImg(this)" />*@
                    </div>
                </div>


                <div class="layui-form-item layui-hide">
                    <label class="layui-form-label">主体右配图</label>
                    <div class="layui-input-block" style="padding-right: 110px;">
                        <input type="text" class="layui-input" name="F_LiveBroadcastId" id="F_LiveBroadcastId" maxlength="255">
                        <button type="button" class="layui-btn" style="position: absolute;top: 0;right: 0;cursor: pointer;" id="j_upload_img_btnRight">
                            <i class="layui-icon"></i>上传图片
                        </button>
                        <ul id="upload_img_wrap"></ul>
                        <textarea id="uploadEditorRight" style="display: none;"></textarea>
                        <span style="color:red">(714*1070)上传图片不能大于1M</span>
                    </div>
                </div>
                <div class="layui-form-item layui-hide" id="F_LiveBroadcastIdImgshow">
                    <label class="layui-form-label"></label>
                    <div class="layui-input-block">
                        <img id="F_LiveBroadcastIdPic" onclick="previewImg(this)" />
                    </div>
                </div>


                <div class="layui-form-item layui-hide">
                    <label class="layui-form-label">主体文字</label>
                    <div class="layui-input-block">
                        <textarea id="F_ShareDes" name="F_ShareDes" type="text/plain" style="width:100%;height:400px;"></textarea>

                    </div>
                </div>

                <div class="layui-form-item layui-hide">
                    <label class="layui-form-label">主体外链</label>
                    <div class="layui-input-block">
                        <input type="text" id="F_TitleLink" name="F_TitleLink" class="layui-input">
                    </div>
                </div>


                @*<div class="layui-form-item layui-hide">
                        <label class="layui-form-label required">简介</label>
                        <div class="layui-input-block">
                            <textarea id="F_LiveIntroduction" name="F_LiveIntroduction" type="text/plain" style="width:100%;height:400px;"></textarea>
                        </div>
                    </div>*@


                <div class="layui-form-item layui-hide">
                    <label class="layui-form-label">排序</label>
                    <div class="layui-input-block">
                        <input type="number" pattern="[0-9]*" id="F_Order" name="F_Order" lay-verify="required" class="layui-input" value="0">
                    </div>
                </div>
                <div class="layui-form-item layui-hide" style="display:none;">
                    <label class="layui-form-label">选项</label>
                    <div class="layui-input-block">
                        <input type="checkbox" name="F_EnabledMark" id="F_EnabledMark" checked="" value="true" title="有效">
                        <input id="F_IsTop" name="F_IsTop" type="checkbox" value="true" title="是否置顶">
                    </div>
                </div>
                <div class="layui-form-item layui-hide">
                    <label class="layui-form-label">发布时间</label>
                    <div class="layui-input-block">
                        <input type="text" id="F_PublishTime" name="F_PublishTime" autocomplete="off" maxlength="200" class="layui-input">
                    </div>
                </div>
                <div class="layui-form-item layui-hide" style="display:none;">
                    <label class="layui-form-label">审核状态</label>
                    <div class="layui-input-block">
                        <input type="checkbox" name="F_Status" id="F_Status" checked="" value="true" title="审核通过">
                    </div>
                </div>


                <div class="layui-form-item layui-hide">
                    <label class="layui-form-label">Seo标题</label>
                    <div class="layui-input-block">
                        <input type="text" id="F_SeoTitle" name="F_SeoTitle" autocomplete="off" class="layui-input" placeholder="不填写则默认为网站标题">
                    </div>
                </div>
                <div class="layui-form-item layui-hide">
                    <label class="layui-form-label">Seo关键字</label>
                    <div class="layui-input-block">
                        <input type="text" id="F_SeoKeywords" name="F_SeoKeywords" autocomplete="off" class="layui-input" placeholder="不填写则默认为网站摘要">
                    </div>
                </div>
                <div class="layui-form-item layui-hide">
                    <label class="layui-form-label">Seo描述</label>
                    <div class="layui-input-block">
                        <textarea id="F_SeoDescription" name="F_SeoDescription" autocomplete="off" class="layui-textarea" placeholder="不填写则自动截取内容前255字符"></textarea>
                    </div>
                </div>

                <div class="form-group-bottom text-right">
                    <div class="layui-input-block">
                        <button class="layui-btn site-demo-active" lay-submit lay-filter="saveBtn">
                            &emsp;保存&emsp;
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>

